{
    "name": "yanmarques/cloudpaths",
    "description": "A mapper to create paths for cloud storage",
    "type": "project",
    "license": "MIT",
    "url": "https://github.com/yanmarques/cloudpaths",
    "authors": [
        {
            "name": "Yan Marques",
            "email": "marques_yan@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "illuminate/config": "5.5.*|5.6.*",
        "illuminate/pipeline": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "illuminate/container": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Cloudpaths\\": "src/Cloudpaths"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Cloudpaths\\": "tests/"
        }
    },
    "files": [
        "vendor/illuminate/support/helpers.php"
    ],
    "minimum-stability": "stable"
}
